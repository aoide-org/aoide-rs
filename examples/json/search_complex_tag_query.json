{
    "filter": {
        "all": [
            {
                "any": [
                    {
                        "tag": {
                            "facet": "genre",
                            "label": {
                                "equals": "R&B/Soul"
                            },
                            "score": {
                                "gt": 0.8
                            }
                        }
                    },
                    {
                        "tag": {
                            "facet": "genre",
                            "label": {
                                "containsNot": "rap"
                            },
                            "score": {
                                "lt": 0.7
                            }
                        }
                    }
                ]
            },
            {
                "any": [
                    {
                        "tag": {
                            "facet": "event",
                            "label": {
                                "matches": "bday"
                            }
                        }
                    },
                    {
                        "tag": {
                            "facet": "event",
                            "label": {
                                "matches": "xmas"
                            }
                        }
                    }
                ]
            },
            {
                "tag": {
                    "facet": "epoch",
                    "label": {
                        "startsNotWith": "19"
                    }
                }
            },
            {
                "not": {
                    "numeric": {
                        "field": "musicTempo",
                        "value": {
                            "gt": 80
                        }
                    }
                }
            }
        ]
    },
    "ordering": [
        {
            "field": "lastRevisionedAt",
            "direction": "dsc"
        }
    ]
