{
    "filter": {
        "all": [
            {
                "any": [
                    {
                        "tag": {
                            "facet": "genre",
                            "label": {
                                "matcher": {
                                    "equals": "R&B/Soul"
                                }
                            },
                            "score": {
                                "gt": 0.8
                            }
                        }
                    },
                    {
                        "tag": {
                            "facet": "genre",
                            "label": {
                                "modifier": "not",
                                "matcher": {
                                    "contains": "rap"
                                }
                            },
                            "score": {
                                "lt": 0.7
                            }
                        }
                    }
                ]
            },
            {
                "any": [
                    {
                        "tag": {
                            "facet": "event",
                            "label": {
                                "matcher": {
                                    "matches": "bday"
                                }
                            }
                        }
                    },
                    {
                        "tag": {
                            "facet": "event",
                            "label": {
                                "matcher": {
                                    "matches": "xmas"
                                }
                            }
                        }
                    }
                ]
            },
            {
                "tag": {
                    "facet": "epoch",
                    "label": {
                        "matcher": {
                            "startsWith": "20"
                        }
                    }
                }
            },
            {
                "not": {
                    "numeric": {
                        "field": "musicTempo",
                        "value": {
                            "gt": 80
                        }
                    }
                }
            }
        ]
    },
    "ordering": [
        {
            "field": "lastRevisionedAt",
            "direction": "dsc"
        }
    ]
}